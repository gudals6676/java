package ClassMethod;

import java.util.Scanner;

public class Minus영주 {

	public void min() {
		
        Scanner sc = new Scanner(System.in);
        System.out.println("정수 두 개를 입력하세요.");
		System.out.print("정수1 : ");
        int num1 = sc.nextInt();
        System.out.print("정수2 : ");
        int num2 = sc.nextInt();
        
        System.out.println(num1 - num2);
	}

}
